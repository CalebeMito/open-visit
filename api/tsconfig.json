{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "node",
    "lib": ["esnext"],
    "types": ["@cloudflare/workers-types"],
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": ["app.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
