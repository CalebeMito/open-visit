{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "esnext",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "lib": ["esnext"],
    "types": ["@cloudflare/workers-types"],
	"allowImportingTSExtensions": true,
	"noEmit": true
  },
  "include": ["app.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
